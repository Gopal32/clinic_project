<!DOCTYPE html>
<html>
    <body>
        <table width="100%">
            <tr>
                <th>SR no</th>
                <th>speciality</th>
                <th>edit / delete</th>
            </tr>
            <% user.forEach(function(user) { %>
            <tr>
                <td><%= user.Sr_no %></td>
                <td><%= user.speciality %></td>
                <td>
                    <a href="/edit/<%= user.Sr_no %>"><button>Edit</button></a>
                    <a href="/delete/<%= user.Sr_no %>"><button>delete</button></a>
                </td>
            </tr>
            <% }) %>
        </table>

        <a href="/speciality"><button>speciality</button></a>
        <a href="/doctor_add"><button>doctor</button></a>
    </body>
</html>